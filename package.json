{
  "name": "feedo",
  "version": "0.3.1",
  "type": "module",
  "description": "An RSS/Atom feed reader that runs on your laptop or on almost any free hosting provider or server.",
  "keywords": [
    "feed",
    "reader",
    "rss",
    "atom",
    "news"
  ],
  "scripts": {
    "dev": "dotenv -e .env -- run-s migrate:dev dev:start",
    "dev:start": "run-p -l dev:watch:*",
    "dev:watch:server": "nodemon",
    "dev:watch:assets": "vite",
    "start:acceptance": "dotenv -e .env.test -- run-s build destroy:testdb migrate:dev start",
    "destroy:testdb": "dotenv -e .env.test -- rm -f ${FEEDO_DATABASE_URL}",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:acceptance": "run-p -r start:acceptance cypress:run",
    "cypress:run": "dotenv -e .env.test -- cypress run",
    "test": "run-s lint test:unit test:acceptance",
    "lint": "eslint --ext .js .",
    "migrate:dev": "prisma migrate dev",
    "migrate:production": "prisma migrate deploy",
    "feed:sync": "dotenv node bin/feedo.js sync",
    "build": "NODE_ENV=production  run-p build:*",
    "build:assets": "vite build",
    "deploy": "dotenv run-s deploy:setup deploy:push",
    "deploy:setup": "git remote | grep deploy || git remote add deploy ${DEPLOY_USER:-feedo}@${DEPLOY_HOST:-feedo}:${DEPLOY_REPO:-feedo}",
    "deploy:push": "git push --force deploy HEAD:main",
    "prepack": "npm run build",
    "cypress:open": "dotenv -- cypress open",
    "start": "node ./bin/feedo.js start"
  },
  "prisma": {
    "schema": "app/services/db/schema.prisma"
  },
  "author": {
    "email": "matiassurdi@gmail.com",
    "name": "MatÃ­as Surdi",
    "url": "https://github.com/msurdi"
  },
  "homepage": "https://github.com/msurdi/feedo",
  "bugs": {
    "email": "matiassurdi@gmail.com",
    "url": "https://github.com/msurdi/feedo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msurdi/feedo.git"
  },
  "license": "MIT",
  "bin": {
    "feedo": "bin/feedo.js"
  },
  "files": [
    "app/**",
    "bin/**",
    "public/**",
    "cli/**"
  ],
  "engines": {
    "node": ">=19",
    "npm": ">=7"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-postcss": "^0.17.2",
    "@faker-js/faker": "^7.6.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/express": "^4.17.13",
    "autoprefixer": "^10.4.0",
    "browser-sync": "^2.27.7",
    "browsersync": "^0.0.1-security",
    "cssnano": "^5.0.6",
    "cypress": "^12.4.1",
    "dotenv-cli": "^7.0.0",
    "esbuild-plugin-postcss2": "^0.1.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.3.1",
    "tailwindcss": "^3.0.23",
    "unpoly": "^2.0.1",
    "vite": "^4.1.1",
    "vite-plugin-full-reload": "^1.0.5",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.3.1",
    "commander": "^10.0.0",
    "compression": "^1.7.4",
    "content-type": "^1.0.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^2.0.0",
    "date-fns": "^2.22.1",
    "downsize": "^0.0.8",
    "execa": "^5.1.1",
    "expand-home-dir": "^0.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.0",
    "find-root": "^1.1.0",
    "fs-extra": "^11.1.0",
    "helmet": "^6.0.1",
    "html-string": "^3.0.1",
    "http-status-codes": "^2.1.4",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "parse-duration": "^1.0.0",
    "path-to-regexp": "^6.2.0",
    "prisma": "^4.9.0",
    "randomstring": "^1.2.1",
    "rss-parser": "^3.12.0",
    "sanitize-html": "^2.4.0",
    "top-user-agents": "^1.0.50",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  }
}
