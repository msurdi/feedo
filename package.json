{
  "name": "feedo",
  "version": "0.3.1",
  "description": "An RSS/Atom feed reader that runs on your laptop or on almost any free hosting provider or server.",
  "type": "module",
  "keywords": [
    "feed",
    "reader",
    "rss",
    "atom",
    "news"
  ],
  "scripts": {
    "dev": "dotenv -e .env -- run-s migrate:dev dev:start",
    "dev:start": "next dev",
    "start:acceptance": "dotenv -e .env.test -- run-s build destroy:testdb migrate:dev start",
    "destroy:testdb": "dotenv -e .env.test -- rm -f ${FEEDO_DATABASE_URL}",
    "test:unit": "jest --verbose server/** cli/**",
    "test:unit:watch": "jest --watch --verbose server/** cli/**",
    "test:acceptance": "run-p -r start:acceptance cypress:run",
    "cypress:run": "dotenv -e .env.test -- cypress run",
    "test": "run-s lint test:unit test:acceptance",
    "lint": "eslint --ext .js .",
    "migrate:dev": "prisma migrate dev",
    "migrate:production": "prisma migrate deploy",
    "feed:sync": "node bin/feedo.js sync",
    "build": "NODE_ENV=production  run-p build:*",
    "build:js": "esbuild --bundle --sourcemap --minify --outdir=public/dist/ assets/app.js",
    "build:css": "postcss assets/app.css --dir public/dist/",
    "deploy:dokku": "dotenv run-s deploy:dokku:setup deploy:dokku:push",
    "deploy:dokku:setup": "git remote | grep dokku || git remote add dokku ${DOKKU_USER:-dokku}@${DOKKU_HOST:-example.com}:${DOKKU_APP:-feedo}",
    "deploy:dokku:push": "git push --force dokku main:master",
    "prepack": "npm run build",
    "cypress:open": "dotenv -e .env.test -- cypress open",
    "start": "node ./bin/feedo.js start"
  },
  "prisma": {
    "schema": "lib/services/db/schema.prisma"
  },
  "author": {
    "email": "matiassurdi@gmail.com",
    "name": "MatÃ­as Surdi",
    "url": "https://github.com/msurdi"
  },
  "homepage": "https://github.com/msurdi/feedo",
  "bugs": {
    "email": "matiassurdi@gmail.com",
    "url": "https://github.com/msurdi/feedo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msurdi/feedo.git"
  },
  "license": "MIT",
  "bin": {
    "feedo": "bin/feedo.js"
  },
  "files": [
    "server/**",
    "bin/**",
    "public/**",
    "cli/**"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-postcss": "^0.15.10",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "autoprefixer": "^10.4.4",
    "browser-sync": "^2.27.7",
    "browsersync": "^0.0.1-security",
    "cssnano": "^5.0.6",
    "cypress": "^9.1.0",
    "dotenv-cli": "^5.0.0",
    "esbuild": "^0.14.27",
    "esbuild-plugin-postcss2": "^0.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.3.1",
    "tailwindcss": "^3.0.23",
    "unpoly": "^2.0.1"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0",
    "@prisma/client": "^3.1.1",
    "classnames": "^2.3.1",
    "commander": "^9.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "date-fns": "^2.22.1",
    "downsize": "^0.0.8",
    "eslint-config-airbnb": "^19.0.4",
    "execa": "^5.1.1",
    "expand-home-dir": "^0.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.0",
    "find-root": "^1.1.0",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.2",
    "html-string": "^3.0.1",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "next": "^12.1.0",
    "next-connect": "^0.12.2",
    "parse-duration": "^1.0.0",
    "path-to-regexp": "^6.2.0",
    "prisma": "^3.1.1",
    "randomstring": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "react-use": "^17.3.2",
